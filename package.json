{
  "name": "app-rest-service",
  "version": "0.0.1",
  "commit": "LOCAL",
  "private": true,
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "PORT=3001 mocha ./src/**/*.spec.js",
    "eslint": "./node_modules/.bin/eslint src/",
    "coverage": "PORT=3001 ./node_modules/.bin/istanbul cover -x **/*.spec.js --report html node_modules/mocha/bin/_mocha "
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "before",
      "beforeEach"
    ]
  },
  "dependencies": {
    "auth0": "^2.6.0",
    "body-parser": "^1.16.0",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "stripe": "^4.15.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "chai-json-schema": "^1.4.0",
    "eslint": "^3.1.1",
    "istanbul": "^1.1.0-alpha.1",
    "jsonwebtoken": "^7.2.1",
    "mocha": "^3.2.0",
    "node-mocks-http": "^1.5.6",
    "nodemon": "^1.9.2",
    "rewire": "^2.5.2"
  }
}
