{
  "name": "app-rest-service",
  "version": "0.0.1",
  "commit": "LOCAL",
  "private": true,
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "PORT=3001 LOGGER_LEVEL=error mocha ./src/**/*.spec.js",
    "eslint": "./node_modules/.bin/eslint src/",
    "coverage": "PORT=3001 LOGGER_LEVEL=error ./node_modules/.bin/istanbul cover -x **/*.spec.js --report html node_modules/mocha/bin/_mocha",
    "codacy-coverage": "PORT=3001 LOGGER_LEVEL=error istanbul cover -x **/*.spec.js _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after",
      "afterEach"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.30.0",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.16.0",
    "bull": "^2.2.6",
    "cors": "^2.8.1",
    "cuid": "^1.3.8",
    "es6-promise": "^4.1.0",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "express-validator": "^3.1.3",
    "formidable": "^1.1.1",
    "lodash": "^4.17.4",
    "stripe": "^4.15.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "chai-json-schema": "^1.4.0",
    "codacy-coverage": "^2.0.2",
    "eslint": "^3.1.1",
    "istanbul": "^1.1.0-alpha.1",
    "jsonwebtoken": "^7.2.1",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "node-mocks-http": "^1.5.6",
    "nodemon": "^1.9.2",
    "rewire": "^2.5.2",
    "sinon": "^2.1.0"
  }
}
